{
  "manifest_version": 2,
  "name": "Y-Downloader",
  "version": "0.1",
  "page_action": {
 "default_icon": "icon.png"

},
 "permissions": [
          "tabs"
        ],

  "content_scripts": [
    {
              "matches": ["*://www.youtube.com/*"],

      "js": ["content.js"]
    }
  ],
  "background": {
  "scripts": ["background.js"]
}
  
}
